{
  "name": "povs/lister-twig-bundle",
  "type": "symfony-bundle",
  "description": "Twig implementation for lister bundle. Provides Twig and Ajax list types for full stack web applications",
  "keywords": ["lister", "list", "twig", "ajax", "symfony", "bundle"],
  "license": "MIT",
  "authors": [
    {
      "name": "Povilas Margaitis",
      "homepage": "https://github.com/povs"
    }
  ],

  "require": {
    "php":                                "^7.1",
    "symfony/twig-bundle":                "^4.0 || ^5.0",
    "povs/lister-bundle":                 "@dev"
  },

  "require-dev": {
    "phpunit/phpunit": "^7",
    "symfony/yaml": "^3.4 || ^4.0",
    "dg/bypass-finals": "^1.1"
  },

  "autoload": {
    "psr-4": {"Povs\\ListerTwigBundle\\": "src/"}
  },

  "autoload-dev": {
    "psr-4": {"Povs\\ListerTwigBundle\\": "tests/"}
  },

  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
